{
    "useTabChar": true,
    "spaceUnits": 4,
    "highlightMatches": true,
    "path": {
        "**.js": {
            "useTabChar": false,
            "spaceUnits": 2,
            "linting.enabled": true,
            "linting.collapsed": false,
            "fonts.fontSize": "12px",
            "mikaeljorhult.bracketsPHPLintTools.php-available": true
        },
        "**.jsx": {
            "useTabChar": false,
            "spaceUnits": 2,
            "linting.enabled": true,
            "linting.collapsed": false,
            "fonts.fontSize": "12px",
            "mikaeljorhult.bracketsPHPLintTools.php-available": true
        },
        "**.json": {
            "useTabChar": false,
            "spaceUnits": 2,
            "linting.enabled": true,
            "linting.collapsed": false,
            "fonts.fontSize": "12px",
            "mikaeljorhult.bracketsPHPLintTools.php-available": true
        }
    },
    "dimitrk.exclude-indexing-filetree.excludeList": [
        ".git(?!ignore)",
        "build",
        "vendor",
        "node_modules"
    ],
    "language": {
        "javascript": {
            "linting.prefer": [
                "ESLint"
            ],
            "linting.usePreferredOnly": true
        }
    },
    "themes.theme": "dark-theme"
}